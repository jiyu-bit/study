#include <stdio.h>
#include <string.h>
#include <windows.h>
//모두 do~while문과 for문을 이용해서 해결하세요
//
//1. do~while문을 이용해서 1부터 100까지 출력하세요
//for문을 이용해서 1부터 100까지 출력하세요

void main_01()
{
	for (int i = 1; i <= 100; i++)
	{
		printf("%d \n", i);
	}

}
//2. do~while문을 이용해서 10부터 20까지 출력하세요
//for문을 이용해서 10부터 20까지 출력하세요
void main_02()
{
	for (int i = 1; i <= 20; i++)
	{
		printf("%d \n", i);
	}


}
//3. do~while문을 이용해서 1부터 10까지 합을 출력하세요
//for문을 이용해서 1부터 10까지 합을 출력하세요

void main_03() {
	int sum = 0;
	int i = 1;

	for (i = 1; i <= 10; i++) {
		sum += i;
	}
	
		printf("1~10까지의 합은 >> %d \n", sum);
	

}

